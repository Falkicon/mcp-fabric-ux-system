{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["_docs_fabric_ux/**"],
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "X-API-Key, Content-Type, Accept"
      }
    }
  ],
  "functions": {
    "src/index.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "buildCommand": "chmod +x ./vercel-build.sh && ./vercel-build.sh",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "enabled": true,
    "silent": false
  }
}